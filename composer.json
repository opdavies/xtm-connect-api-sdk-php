{
    "name": "opdavies/xtm-connect-api-sdk-php",
    "description": "An SDK for interacting with the XTM Connect REST API.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Opdavies\\XtmConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Opdavies\\XtmConnect\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Oliver Davies"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "symfony/http-client": "^6.2",
        "webmozart/assert": "^1.11"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    }
}
